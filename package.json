{
  "name": "etheralley-web-interface",
  "private": true,
  "scripts": {
    "start": "craco start",
    "start:prod": "serve -s build",
    "build": "craco build",
    "analyze": "source-map-explorer 'build/static/js/*.js' --html analyze/result.html",
    "test": "craco test --watchAll",
    "test:nowatch": "craco test --watchAll=false",
    "extract": "formatjs extract 'src/**/*.ts*' --ignore='**/*.d.ts' --out-file lang/en.json",
    "compile-folder": "formatjs compile-folder --ast lang src/providers/IntlProvider/lang",
    "compile-pseudos": "formatjs compile lang/en.json --out-file src/providers/IntlProvider/lang/ro.json --ast --pseudo-locale xx-LS && formatjs compile lang/en.json --out-file src/providers/IntlProvider/lang/hr.json --ast --pseudo-locale en-XA",
    "intl": "npm run extract && npm run compile-folder && npm run compile-pseudos"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.3.6",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@metamask/detect-provider": "^1.0.0",
    "@reduxjs/toolkit": "^1.8.6",
    "decimal.js": "^10.4.2",
    "ethers": "^5.7.2",
    "framer-motion": "^7.6.1",
    "nanoid": "^3.3.4",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "react-icons": "^4.6.0",
    "react-intl": "^6.2.1",
    "react-redux": "^8.0.4",
    "react-router-dom": "^6.4.2",
    "react-scripts": "5.0.1",
    "wagmi": "^0.7.5"
  },
  "devDependencies": {
    "@craco/craco": "^7.0.0-alpha.8",
    "@formatjs/cli": "^5.1.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.0",
    "@types/node": "^18.11.2",
    "@types/react": "^18.0.21",
    "@types/react-beautiful-dnd": "^13.1.2",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "^5.3.3",
    "serve": "^14.0.1",
    "source-map-explorer": "^2.5.3",
    "typescript": "~4.8.4",
    "webpack": "^5.74.0"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {}
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
